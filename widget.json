{
  "name": "List (panels)",
  "package": "com.fliplet.panels",
  "version": "1.0.0",
  "icon": "img/icon.png",
  "tags": ["type:component", "category:panels"],
  "provider_only": false,
  "html_tag": "div",
  "providers": [
    "com.fliplet.link",
    "com.fliplet.image-manager"
  ],
  "interface": {
    "dependencies": [
      "fliplet-core",
      "fliplet-studio-ui",
      "jquery-ui",
      "lodash",
      "handlebars",
      "bootstrap"
    ],
    "assets": [
      "vendor/bootstrap-colorpicker.min.js",
      "vendor/bootstrap-colorpicker.css",
      "js/interface.js",
      "css/interface.css"
    ]
  },
  "build": {
    "dependencies": [
      "fliplet-core",
      "bootstrap-css",
      "lodash"
    ],
    "assets": [
      "js/build.js",
      "css/build.css"
    ]
  },
  "references": [
    "items.$.linkAction.page:page",
    "items.$.linkAction.files.id:mediaFile",
    "items.$.imageConf.id:mediaFile"
  ]
}
